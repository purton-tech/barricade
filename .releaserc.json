{
    "branches": ["master"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      ["@semantic-release-plus/docker", {
          "name": "authnproxy/authnproxy:latest",
          "skipLogin": true
      }],
      ["@semantic-release/exec", {
        "publishCmd": "./update-config.sh",
        "execCwd": "./infra"
      }]
    ]
  }
  